{
  "manifest_version": 3,
  "name": "AI Prompt Assistant",
  "version": "3.0",
  "description": "Add custom prompt shortcuts to ChatGPT, Claude, Gemini, DuckDuckGo, and Microsoft Copilot with autocomplete functionality",
  "permissions": [
    "storage",
    "notifications",
    "identity"
  ],
  "key":"",
  "oauth2": {
    "client_id": "811759403062-oilssdj6ou5jdjv9ht4ko2tl384bre09.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*", 
        "https://chat.openai.com/*",
        "https://claude.ai/*",
        "https://gemini.google.com/*",
        "https://aistudio.google.com/*",
        "https://duckduckgo.com/*",
        "https://m365.cloud.microsoft/*",
        "https://copilot.microsoft.com/*"
      ],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_title": "AI Prompt Assistant"
  },
  "web_accessible_resources": [
    {
      "resources": ["debug-oauth.html", "debug-oauth.js", "test-google-drive.html", "test-google-drive.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}