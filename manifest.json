{
  "manifest_version": 3,
  "name": "ChatGPT Prompt Assistant",
  "version": "1.0",
  "description": "Add custom prompt shortcuts to ChatGPT with autocomplete functionality",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*", "https://chat.openai.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_title": "ChatGPT Prompt Assistant"
  }
}